\n  type: 'DELETE_MOVIE',\r\n  payload: { title },\r\n});\r\n\r\nexport const startDeleteMovie = title => (dispatch, getState) => {\r\n  const updatedList = getState().movies.filter(mov => mov.title !== title);\r\n  dispatch(deleteMovie(title));\r\n  axios({\r\n    url: '/updatedata',\r\n    method: 'POST',\r\n    data: updatedList,\r\n  });\r\n};\r\n\r\nexport const setUser = userInfo => ({\r\n  type: 'SET_USER',\r\n  payload: userInfo,\r\n});\r\n"},"lineCount":null}},"hash":"32737d0f5bc4b4c92f5a74f81890eb0e","cacheData":{"env":{}}}